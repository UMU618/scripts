Windows Registry Editor Version 5.00

[HKEY_CURRENT_USER\Software\Classes\*\shell\Copy Path]
@="Copy Path(&Y)"

[HKEY_CURRENT_USER\Software\Classes\*\shell\Copy Path\Command]
@="cmd.exe /c echo %L| clip.exe"

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\Copy Path]
@="Copy Path(&Y)"

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\Copy Path\Command]
@="cmd.exe /c echo %L| clip.exe"

